[alias]
    tree = log --oneline --decorate --all --graph
    co = checkout
    br = branch
    st = status
    unstage = reset HEAD --
    last = log -1 HEAD
    visual = !gitk
    ci = commit
	cloner = clone --recursive
	history = log -p --
[push]
    default = simple
[core]
    editor = nvim
[merge]
	ff = false
[rebase]
    autoStash = true
