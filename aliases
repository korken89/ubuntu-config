alias install="sudo apt install"
alias update="sudo apt update"
alias upgrade="sudo apt update; sudo apt upgrade; rustup update"
alias remove="sudo apt remove"
alias search="apt search"
alias ack="ag -Q"
alias rtlatex="latexmk -pvc -pdf"
alias sr="source devel/setup.zsh"
alias v=vi
alias pullr="bash -c \"find . -name \".git\" -type d | sed 's/\/.git//' |  xargs -P10 -I{} git -C {} pull\""
alias gdb-bmp='arm-none-eabi-gdb -ex "target extended-remote /dev/ttyBmpGDB" -ex "monitor connect_srst enable" -ex "monitor swdp_scan" -ex "attach 1"'
alias nwr="sudo service network-manager restart"
